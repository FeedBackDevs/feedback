<?xml version="1.0" encoding="utf-8"?>
<Frame id="songselect" align="Fill">
	<Frame id="playerpanel" align="TopFill" height="60" background_colour="black">
	</Frame>
	<Label align="TopCenter" text="Song Select" text_height="60" text_colour="yellow" />
	<Listbox id="songlist" align="Center" size="600,600" background_colour="black" list="db.library.songlist_base" hoverSelect="true" OnSelChanged="screen:selectSong(item)" OnClick="screen:playSong(item)" />
</Frame>
